    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "mocha": "^10.2.0",
    "sinon": "^17.0.0",
    "typescript": "^5.3.3",
    "user-agent-data-types": "^0.4.2"
